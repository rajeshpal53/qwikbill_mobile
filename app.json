{
  "expo": {
    "name": "Wertone Billing Center",
    "slug": "wertone-billing-center",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-wertone.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo-wertone.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
        }
      ]
    ],
    "ios": {
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-wertone.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "USE_FINGERPRINT",
        "USE_BIOMETRIC"
      ],
      "package": "com.prathmeshlakhade.wertonebillingcenter"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3e4bc5ea-58d0-4be6-8574-57452ad669a5"
      }
    }
  }
}
