{
  "expo": {
    "name": "Invoicely",
    "slug": "wertonebillingcenter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-wertone.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo-wertone.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
        }
      ]
    ],
    "ios": {
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-wertone.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "USE_FINGERPRINT",
        "USE_BIOMETRIC"
      ],
      "package": "com.faizanshaikh0.wertonebillingcenter"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "50391450-117b-47e6-805e-9097ed8c9492"
      }
    }
  }
}
